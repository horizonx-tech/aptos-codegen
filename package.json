{
  "name": "@horizonx/aptos-codegen",
  "description": "Generate interface for aptos modules from ABI",
  "keywords": [
    "aptos",
    "TypeScript"
  ],
  "version": "0.12.0",
  "author": "HorizonX",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/horizonx-tech/aptos-codegen.git"
  },
  "homepage": "https://leizd.com/devcentral",
  "bin": {
    "aptos-codegen": "dist/cli/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "ci": "yarn clean && yarn coverage && jest-coverage-badges output ./badges",
    "build": "yarn clean && tsc -p tsconfig.build.json && tsc-alias",
    "test": "jest --watch",
    "coverage": "jest --coverage",
    "exe": "ts-node -r tsconfig-paths/register src/cli/index.ts",
    "clean": "rimraf dist coverage badges"
  },
  "dependencies": {
    "aptos": "^1.3.15",
    "change-case": "^4.1.2",
    "glob": "^8.0.3",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@horizonx/aptos-module-client": "^0.5.0",
    "@swc/core": "^1.2.242",
    "@swc/jest": "^0.2.22",
    "@types/glob": "^7.2.0",
    "@types/jest": "^28.1.8",
    "@types/node": "^18.7.9",
    "@types/yargs": "^17.0.11",
    "jest": "^29.0.0",
    "jest-coverage-badges": "^1.1.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "@horizonx/aptos-module-client": ">= 0.5.0",
    "aptos": ">= 1.3.15"
  }
}
